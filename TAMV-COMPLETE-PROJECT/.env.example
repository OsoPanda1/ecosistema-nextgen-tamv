# TAMV Core API - Variables de Entorno
# Copia este archivo a .env y configura los valores

# Configuración de la aplicación
NODE_ENV=production
PORT=3000
LOG_LEVEL=info

# Base de datos PostgreSQL
POSTGRES_HOST=tamv-postgres-production.cluster-xyz.us-west-2.rds.amazonaws.com
POSTGRES_PORT=5432
POSTGRES_DB=tamv_production
POSTGRES_USER=tamv_admin
POSTGRES_PASSWORD=REPLACE_WITH_SECURE_PASSWORD

# Redis Cache
REDIS_HOST=tamv-redis-production.abc123.cache.amazonaws.com
REDIS_PORT=6379
REDIS_PASSWORD=REPLACE_WITH_SECURE_PASSWORD

# Seguridad
JWT_SECRET=REPLACE_WITH_SECURE_JWT_SECRET
BCRYPT_ROUNDS=12
ALLOWED_ORIGINS=https://tamv.org,https://api.tamv.org

# TAMV Ecosystem Configuration
TAMV_ENVIRONMENT=production
TAMV_CLUSTER_NAME=tamv-production
TAMV_REGION=us-west-2

# Servicios integrados
XR_SERVICE_URL=http://tamv-xr-renderer:8080
QUANTUM_SERVICE_URL=http://tamv-quantum-processor:9000
ISABELLA_SERVICE_URL=http://tamv-isabella-ai:7000
BLOCKCHAIN_SERVICE_URL=http://tamv-blockchain-service:6000
SECURITY_SERVICE_URL=http://tamv-security-service:5000

# Configuración XR/VR
XR_RENDER_QUALITY=ultra
XR_MAX_CONCURRENT_USERS=10000
XR_PHYSICS_ENGINE=quantum-enhanced

# Configuración Quantum Computing
QUANTUM_BACKEND=hybrid-classical-quantum
QUANTUM_SIMULATOR=qiskit-aer
QUANTUM_MAX_QUBITS=1000

# Isabella AI Configuration
ISABELLA_MODEL_VERSION=v2.1
ISABELLA_EXPLANATION_LEVEL=detailed
ISABELLA_ETHICAL_MODE=strict
ISABELLA_API_KEY=REPLACE_WITH_SECURE_KEY

# Blockchain Configuration
BLOCKCHAIN_NETWORKS=ethereum,polygon,solana
MSR_ANCHOR_INTERVAL=3600
BLOCKCHAIN_PRIVATE_KEY=REPLACE_WITH_SECURE_KEY

# Tenochtitlan Security Configuration
TENOCHTITLAN_MODE=active
SECURITY_LEVEL=maximum
AUDIT_MODE=comprehensive

# Monitoreo y métricas
ENABLE_METRICS=true
METRICS_PORT=9090
HEALTH_CHECK_INTERVAL=30